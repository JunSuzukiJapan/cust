enum SomeEnum {
    Foo,
    Bar {
        int x;
        int y;
        int z;
    },
    Zot {
        char a;
        char b;
        char c;
    }
};

int main() {
    enum SomeEnum x = SomeEnum::Foo;;
    int result = 0;

    if let (SomeEnum::Foo = x) {
        result = 1;
    } else {
        result = 0;
    }

    return result;
}