int printf(char* format, ...);

enum Foo {
    Bar (int, int),
    Zot {
        int x;
        int y;
    },
};

Foo foo = Foo::Bar(10, 20);

int main() {
    if let (Foo::Bar ( x, y ) = foo) {
        printf("x: %d\n", x);
        printf("y: %d\n", y);
        int result = x + y;
        printf("x + y: %d\n", result);

        return result;
    }else{
        return 0;
    }
}